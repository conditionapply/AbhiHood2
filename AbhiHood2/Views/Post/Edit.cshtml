                <div class="form-group">
                    <label asp-for="State" class="control-label"></label>
                    <input asp-for="State" class="form-control" />
                    <span asp-validation-for="State" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="ZipCode" class="control-label"></label>
                    <input asp-for="ZipCode" class="form-control" />
                    <span asp-validation-for="ZipCode" class="text-danger"></span>
                </div>
                <div class="form-group d-none">
                    <label asp-for="SysAddress" class="control-label"></label>
                    <input asp-for="SysAddress" class="form-control" />
                    <span asp-validation-for="SysAddress" class="text-danger"></span>
                </div>
                <div class="form-group d-none">
                    <label asp-for="SysCity" class="control-label"></label>
                    <input asp-for="SysCity" class="form-control" />
