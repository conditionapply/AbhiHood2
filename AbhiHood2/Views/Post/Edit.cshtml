@model AbhiHood2.Models.PostedUserData

@{
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
    ViewData["Title"] = "Edit";
}


<div class="section-title">
    <h2>Edit</h2>

</div>
<div class="row content">
