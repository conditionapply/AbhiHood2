                    <input type="submit" value="Save" class="btn btn-primary" /> |  <a asp-action="Index" class="btn btn-primary">Back to List</a>

                </div>
            </div>
           
        </form>
        @section Scripts {
            @{
                await Html.RenderPartialAsync("_ValidationScriptsPartial");
            }
        }
    </div>
</div>


